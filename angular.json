{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "mercat-kzero": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/mercat-kzero",
      "sourceRoot": "projects/mercat-kzero/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/mercat-kzero",
            "index": "projects/mercat-kzero/src/index.html",
            "main": "projects/mercat-kzero/src/main.ts",
            "polyfills": "projects/mercat-kzero/src/polyfills.ts",
            "tsConfig": "projects/mercat-kzero/tsconfig.app.json",
            "aot": true,
            "assets": ["projects/mercat-kzero/src/favicon.ico", "projects/mercat-kzero/src/assets"],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "projects/mercat-kzero/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/mercat-kzero/src/environments/environment.ts",
                  "with": "projects/mercat-kzero/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "mercat-kzero:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "mercat-kzero:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "mercat-kzero:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/mercat-kzero/src/test.ts",
            "polyfills": "projects/mercat-kzero/src/polyfills.ts",
            "tsConfig": "projects/mercat-kzero/tsconfig.spec.json",
            "karmaConfig": "projects/mercat-kzero/karma.conf.js",
            "assets": ["projects/mercat-kzero/src/favicon.ico", "projects/mercat-kzero/src/assets"],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "projects/mercat-kzero/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/mercat-kzero/tsconfig.app.json",
              "projects/mercat-kzero/tsconfig.spec.json",
              "projects/mercat-kzero/e2e/tsconfig.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/mercat-kzero/e2e/protractor.conf.js",
            "devServerTarget": "mercat-kzero:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "mercat-kzero:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "mercat-kzero"
}
